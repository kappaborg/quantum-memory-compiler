# Quantum Dashboard - Environment Variables
# ==========================================

# API Configuration
REACT_APP_API_URL=http://localhost:5001
REACT_APP_DEMO_MODE=false

# IBM Quantum Configuration
REACT_APP_IBM_QUANTUM_TOKEN=your_ibm_quantum_token_here
REACT_APP_IBM_QUANTUM_INSTANCE=ibm_quantum
REACT_APP_IBM_QUANTUM_CHANNEL=ibm_quantum

# Development Configuration
NODE_ENV=development
REACT_APP_ENV=development

# GitHub Pages Deployment
# For production, these will be set via GitHub Actions
REACT_APP_GITHUB_PAGES=false
PUBLIC_URL=/quantum-memory-compiler
